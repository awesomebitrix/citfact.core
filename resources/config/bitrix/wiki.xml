<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bx.wiki" class="CWiki"/>
        <service id="bx.wiki.parser" class="CWikiParser"/>
        <service id="bx.wiki.security" class="CWikiSecurity"/>
        <service id="bx.wiki.socnet" class="CWikiSocnet"/>
        <service id="bx.wiki.diff" class="CWikiDiff"/>
        <service id="bx.wiki.utils" class="CWikiUtils"/>
    </services>
</container>