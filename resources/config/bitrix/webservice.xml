<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bx.webservice.xml_creator" class="CXMLCreator"/>
        <service id="bx.webservice.soap_fault" class="CSOAPFault"/>
        <service id="bx.webservice.soap_request" class="CSOAPRequest"/>
        <service id="bx.webservice.soap_server_responser" class="CSOAPServerResponser"/>
        <service id="bx.webservice.soap_server" class="CSOAPServer"/>
        <service id="bx.webservice.soap_client" class="CSOAPClient"/>
        <service id="bx.webservice.wsdl_creator" class="CWSDLCreator"/>
        <service id="bx.webservice.soap_codec" class="CSOAPCodec"/>
        <service id="bx.webservice.soap_response" class="CSOAPResponse"/>
        <service id="bx.webservice.wssoap_responser" class="CWSSOAPResponser"/>
    </services>
</container>