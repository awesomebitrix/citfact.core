    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "citfact/core",
                "version": "master",
                "source": {
                    "url": "https://github.com/studiofact/citfact.core",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "composer/installers": "~1.0"
